# Routes
# This file defines all application routes (Higher priority routes first)
# ~~~~

# Home page
GET     /                           controllers.Application.index()

# Login page

# Providers entry points
GET     /login                      controllers.Application.home()
POST    /authenticate     			controllers.Application.login()
GET    /authenticate     			controllers.Application.index()
GET		/logout						controllers.Application.logout()
GET     /not-authorized             securesocial.controllers.ProviderController.notAuthorized


# Map static resources from the /public folder to the /assets URL path
GET     /dealerjs/*file               controllers.Assets.at(path="/public/angularjs", file)
GET     /assets/*file               controllers.Assets.at(path="/public", file)
GET     /dealer/*file               controllers.Assets.at(path="/public/angularjs/app", file)

